{
  "name": "tree-view",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "react-ui-tree": "https://github.com/phodal/react-ui-tree"
  },
  "scripts": {
    "start": "react-scripts start && yarn copyfile",
    "build": "react-scripts build --watch && yarn copyfile",
    "copyfile": "mkdir -p ../../dist/component/tree-view && cp build/static/js/*.js ../../dist/component/tree-view/tree-view.js && cp build/static/css/*.css ../../dist/component/tree-view/tree-view.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
